.navigation-bar-icon {
  text-align: center;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 50rpx;
}

.logo-box {
  width: 100%;
  padding: 80rpx 50rpx;
  box-sizing: border-box;
}

.logo-box .tips {
  margin-top: 50rpx;
  width: 100%;
  text-align: center;
  font-size: 14px;
}

.collapse {
  background-color: #fff;
  border-radius: 2px;
  overflow: hidden;
  margin-bottom: 30rpx;
}

.collapse-card {
  width: 100%;
  padding: 20px;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.collapse-card .title {
}
.collapse-card .iconfont {
  flex: none;
  font-size: 16px;
}
.collapse-list {
  width: 100%;
  padding: 0 20px;
  box-sizing: border-box;
}
.item {
  height: 90rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid rgba(0, 0, 0, 0.1);
}
.item .title{
  flex: none;
  text-align: left;
}
.item .desc{
  flex: auto;
  text-align: right;
  font-size: 14px;
  color: gray;
}
.sub-list {
  width: 100%;
  box-sizing: border-box;
  padding-left: 20px;
}
.list > .item:first-child {
  border: 0;
}
.path-disable {
  color: gray;
  font-size: 14px;
}
