page {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.bottom-area-size {
  width: 100%;
  height: 120rpx;
  box-sizing: content-box;
  padding-bottom: 30px;
}
.bottom-area-position {
  position: fixed;
  bottom: 0;
  left: 0;
}
.popover-inner .tip {
  padding: 0 8px;
  white-space: nowrap;
}
.popover-inner .close {
  width: 20px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  color: #666;
}
